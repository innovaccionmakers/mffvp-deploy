[
    {
        "WorkflowName": "Contributions.ClientOperation.Validation",
        "Rules": [
            {
                "RuleName": "AmountPositive",
                "ErrorMessage": "Amount must be positive",
                "Expression": "input.Amount > 0"
            },
            {
                "RuleName": "DateNotFuture",
                "ErrorMessage": "Date cannot be in the future",
                "RuleExpressionType": "LambdaExpression",
                "Expression": "input.Date <= DateTime.UtcNow"
            }
        ]
    }
]
